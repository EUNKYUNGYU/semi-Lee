<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>


<entry key="selectList">

SELECT 
        QUIZE_NO
        , QUIZE_TITLE
        , QUIZE_CONTENT
        , VOTE
        , CREATE_DATE
        , TO_CHAR(TRUNC(SYSDATE - END_DATE)) DEADLINE
        , CHOICE1
        , CHOICE2
        , CHOICE3
        , CHOICE4
	FROM
        TB_QUIZE_BOARD 
    ORDER BY
        CREATE_DATE DESC

</entry>

<entry key="selectDetail">

	SELECT 
        	ANSWER, ANSWER_DETAIL
    	FROM 
        	TB_QUIZE_ANSWER
        JOIN 
        	TB_QUIZE_BOARD USING(QUIZE_NO) 
    	WHERE 
        	QUIZE_NO = ?
        
</entry>



</properties>

